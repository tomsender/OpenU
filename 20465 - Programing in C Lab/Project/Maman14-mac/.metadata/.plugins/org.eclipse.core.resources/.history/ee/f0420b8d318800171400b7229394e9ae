#ifndef OPERATION_DICTIONARY_H_
#define OPERATION_DICTIONARY_H_



typedef struct operation_info{
	String name;
	int num_of_operands;
	Bool intermid_allowed;
	Bool label_allowed;
	Bool matrix_allowed;
	Bool register_allowed;
}operation_info;





#endif /* OPERATION_DICTIONARY_H_ */
