#include "utils.h"
#include "parser.h"
#define ARRAYSIZE 200


int main(void) {

	FILE * input_file;
	bodyArray parsed;
	int array_size;
	int parsed_size;

	/*END OF DECLERATIONS*/

	puts("!!!Starting tests!!!\n"); /* prints !!!Hello World!!! */

	if (!(input_file = fopen("assembly_input.txt","r"))){
		fprintf(stderr, "unable to find assembly file");
		exit(0);
	}

	parsed=(bodyArray)parse_file(input_file, &parsed_size);
	fclose(input_file);

	array_size=sizeof(parsed)/sizeof(bodyArray);
	printf("sizeof structs: %d\n",array_size);
	print_structs(parsed,array_size);




	printf("\n\n---===doei!===---\n");
	return EXIT_SUCCESS;
}


